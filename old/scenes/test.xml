<yas>
    <scene root = "defaultRoot" axis_length = "3" />

    <views default = "defaultView">
        <perspective id = "defaultPerspective" near = "10" far = "20" angle = "30">
            <from x = "1" y = "3" z = "5"/>
            <to x = "2" y = "4" z = "6"/>
        </perspective>

         <ortho id = "defaultOrtho" near = "10" far = "20" left = "5" right = "15" top = "25" bottom = "0" />
    </views>

    <ambient>
        <ambient r = "0.1" g = "0.1" b = "0.1" a = "1" />
        <background r = "0.8" g = "0.3" b = "0.9" a = "1" />
    </ambient>

    <lights>
        <omni id = "omniDefault" enabled = "1">
            <location x = "0" y = "1" z = "3" w = "4" />
            <ambient r = "0" g = "1" b = "7" a = "4" />
            <diffuse r = "10" g = "1" b = "3" a = "4" />
            <specular r = "0" g = "11" b = "3" a = "4" />
        </omni>
    </lights>

    <textures>
        <texture id = "defaultTexture" file = "swag.jpeg" /> 
    </textures>

    <materials>
        <material id = "defaultMaterial" shininess = "0">
            <emission r = "0" g = "1" b = "2" a = "3" />
            <ambient r = "0" g = "1" b = "2" a = "3" />
            <diffuse r = "0" g = "420" b = "2" a = "3" />
            <specular r = "0" g = "1" b = "2" a = "3" />
        </material>
    </materials>

    <transformations>
        <transformation id = "t1">
        </transformation>
    </transformations>
</yas>